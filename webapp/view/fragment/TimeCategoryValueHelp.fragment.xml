<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" class="sapUiSizeCompact">
	<Dialog title="Work Locations" class="sapUiContentPadding" contentWidth="60rem" horizontalScrolling="false">
		<List items="{path: 'WorkLocations>/'}" mode="SingleSelectMaster" selectionChange="onWLValueHelpSelectedItem">
			<ObjectListItem title="{WorkLocations>SHORT}" number="{WorkLocations>STEXT}"></ObjectListItem>
		</List>
		<buttons>
			<Button id="idTCVHClose" text="Close" press="onWLValueHelpDialogClose"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>