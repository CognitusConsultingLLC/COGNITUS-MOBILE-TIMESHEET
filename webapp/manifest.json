{
  "_version": "1.12.0",
  "sap.app": {
    "id": "cgdc.timesheet",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "servicecatalog.connectivityComponentForManifest",
      "version": "0.0.0",
      "toolsId": "46310c1c-7165-451e-9186-2e60feb89955"
    },
    "dataSources": {
      "GW_HCMFAB_TIMESHEET_MAIN_SRV": {
        "uri": "/sap/opu/odata/cgdc/GW_HCMFAB_TIMESHEET_MAIN_SRV/",
        "type": "OData",
        "settings": {
          "localUri": "localService/GW_HCMFAB_TIMESHEET_MAIN_SRV/metadata.xml",
          "annotations": [
            "annotation0"
          ]
        }
      },
      "annotation0": {
        "type": "ODataAnnotation",
        "uri": "annotation0.xml",
        "settings": {
          "localUri": "annotation0.xml"
        }
      }
    }
  },
  "sap.ui": {
    "fullWidth": true,
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "cgdc.timesheet.view.View1",
      "type": "XML",
      "async": true,
      "id": "View1"
    },
    "dependencies": {
      "minUI5Version": "1.65.6",
      "libs": {
        "sap.f": {},
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "cgdc.timesheet.i18n.i18n"
        }
      },
      "": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingMode": "OneWay",
          "defaultCountMode": "Request"
        },
        "dataSource": "GW_HCMFAB_TIMESHEET_MAIN_SRV",
        "preload": true
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "cgdc.timesheet.view",
        "controlId": "fcl"
      },
      "routes": [
        {
          "name": "default",
          "pattern": "",
          "layout": "OneColumn",
          "target": [
            "main"
          ]
        },
        {
          "name": "detail",
          "pattern": "detail/{index}",
          "titleTarget": "",
          "layout": "TwoColumnsMidExpanded",
          "greedy": false,
          "target": [
            "detail"
          ]
        },
        {
          "name": "detaildetail",
          "pattern": "detaildetail/{index}/{sindex}",
          "titleTarget": "",
          "layout": "ThreeColumnsMidExpanded",
          "greedy": false,
          "target": [
            "detaildetail"
          ]
        }
      ],
      "targets": {
        "TargetView1": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "View1",
          "viewName": "View1"
        },
        "main": {
          "viewType": "XML",
          "viewName": "main",
          "controlAggregation": "beginColumnPages"
        },
        "detail": {
          "viewType": "XML",
          "viewName": "detail",
          "controlAggregation": "midColumnPages"
        },
        "detaildetail": {
          "viewType": "XML",
          "viewName": "detaildetail",
          "controlAggregation": "endColumnPages"
        }
      }
    }
  },
  "sap.platform.abap": {
    "uri": "/sap/bc/ui5_ui5/cgdc/fr_timesheet/webapp",
    "_version": "1.1.0"
  },
  "sap.cloud": {
    "public": true,
    "service": "cgdctimesheet"
  }
}
